    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Homebridge",
            "program": "/usr/local/bin/homebridge",
            "args": [ "--plugin-path=${workspaceRoot}/"],
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "port": 5858
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch make-gpio-script",
            "program": "${workspaceRoot}/make-gpio-script",
            "args": [],
            "cwd": "${workspaceRoot}"
        }
    ]
}